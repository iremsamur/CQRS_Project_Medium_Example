@model List<GetAllDoctorsForPatientQueryResult>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<h1>Hasta Paneli : Doktor Bilgileri</h1>
<div class="container">
    <br>
    <table class="table table-bordered">
        <tr>
        
            <th>Ad</th>
            <th>Soyad</th>
            <th>Hastane Adı</th>
            <th>Poliklinik Adı</th>
   
        </tr>
        @foreach (var item in Model)
        {
            <tr>
             
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.HospitalName</td>
                <td>@item.PoliclinicName</td>
         

              
            </tr>

        }

    </table>
   

</div>

